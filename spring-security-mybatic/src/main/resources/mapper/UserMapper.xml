<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.jbzm.demo.springsecuritymybatic.mapper.UserMapper">
    <select id="findByUserName" resultType="top.jbzm.demo.springsecuritymybatic.entity.User">
        select * from t_user where username = #{username}
    </select>
</mapper>